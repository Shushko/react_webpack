{
  "name": "react-webpack",
  "version": "1.0.0",
  "private": true,
  "description": "React-Webpack App Template",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --hot",
    "build:development": "cross-env APP_ENV=development NODE_ENV=production webpack",
    "build:staging": "cross-env APP_ENV=staging NODE_ENV=production webpack",
    "build:production": "cross-env APP_ENV=production NODE_ENV=production webpack",
    "build:analyze": "npm run build:production -- --env addon=bundleanalyze",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    ">0.5%",
    "not dead"
  ],
